<Page x:Class="B2B_Utils.View.SearchStat"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:B2B_Utils.View"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="600"
      Title="SearchStat">

    <Grid>
        <TextBox Margin="11,40,10,397" x:Name="FileText"/>
        <Button Margin="66,229,392,339" x:Name="FileButton" Click="FileButton_Click" >Load SearchStat Log</Button>
        <Button Margin="448,229,10,339" x:Name="ExportExcelButton" Click="ExportExcelButton_Click">Export in Excel</Button>
        <RadioButton x:Name="ProdB" Content="ProdB" HorizontalAlignment="Left" Margin="10,208,0,0" VerticalAlignment="Top" Height="16"/>
        <RadioButton x:Name="Search1" Content="Search1" HorizontalAlignment="Left" Margin="66,208,0,0" VerticalAlignment="Top" Height="16"/>
        <RadioButton x:Name="Search2" Content="Search2" HorizontalAlignment="Left" Margin="132,208,0,0" VerticalAlignment="Top" Height="16"/>
        <RadioButton x:Name="Search3" Content="Search3" HorizontalAlignment="Left" Margin="198,208,0,0" VerticalAlignment="Top" Height="16"/>
    </Grid>
</Page>
